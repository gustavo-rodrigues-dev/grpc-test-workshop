{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "node": "current"
                }
            }
        ]
    ],
    "env": {
        "test": {
            "plugins": [
                [
                    "istanbul",
                    {
                        "exclude": [
                            "**/*.spec.js"
                        ]
                    }
                ]
            ]
        }
    },
    "sourceMaps": true,
    "retainLines": true
}